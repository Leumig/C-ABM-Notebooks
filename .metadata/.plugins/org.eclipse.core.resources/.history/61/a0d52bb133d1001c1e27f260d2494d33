#ifndef TRABAJO_H_
#define TRABAJO_H_

typedef struct{

int id;
int idNotebook;
int idServicio;
eFecha fPedida;
char descripcion[20];

}eTipo;

• id (autoincremental)
• idNotebook (debe existir) Validar
• idServicio (debe existir) Validar
• fecha (Validar día, mes y año)

#endif /* TRABAJO_H_ */
