#ifndef TRABAJO_H_
#define TRABAJO_H_

typedef struct{

int id;
int idNotebook;
int idServicio;
eFecha fPactada;
char descripcion[20];

}eTrabajo;

int mostrarTrabajo(eTrabajo lista, eNotebook notebooks[], int tamNot);

int listarTrabajoks(eTrabajo vec[], eNotebook notebooks[], int tamNot);


int altaTrabajo(eTrabajo vec[], int tam, int* pId, eNotebook notebooks[], int tamNot);


#endif /* TRABAJO_H_ */
